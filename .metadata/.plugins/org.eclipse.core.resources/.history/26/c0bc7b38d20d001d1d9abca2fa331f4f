package fundamentos.operadores;

public class Unarios {

	public static void main(String[] args) {
		
		int a = 5;
		int b = 10;
		
		a++;  // a = a + 1;
		System.out.println(a);
		
		a--;  // a = a - 1;
		System.out.println(a);
		
		++b;  // b = b + 1;
		System.out.println(b);
		
		--b;  // b = b - 1;
		System.out.println(b);
	}
}
